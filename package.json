    "concurrently": "^5.2.0",
    "jquery": "^3.5.1",
    "ngx-useful-swiper": "^9.0.3",
    "rxjs": "~6.6.0",
    "select2": "^4.0.13",
    "swiper": "^6.0.2",
    "tslib": "^2.0.0",
    "vanilla-lazyload": "^17.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
