{
  "name": "angular-cao-thang",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run ckc-backend\" \"npm run ckc-admin\" \"npm run ckc-cntt\" \"npm run ckc-elearning\" \"npm run ckc-ttth\"",
    "ckc-api-admin": "concurrently \"npm run ckc-backend\" \"npm run ckc-admin\"",
    "ckc-api-cntt": "concurrently \"npm run ckc-backend\" \"npm run ckc-cntt\"",
    "ckc-api-elearning": "concurrently \"npm run ckc-backend\" \"npm run ckc-elearning\"",
    "ckc-api-ttth": "concurrently \"npm run ckc-backend\" \"npm run ckc-ttth\"",
    "ckc-backend": "node projects/backend/server.js",
    "ckc-admin": "ng serve admin --port=4200",
    "ckc-cntt": "ng serve cntt --port=4300",
    "ckc-elearning": "ng serve elearning --port=4400",
    "ckc-ttth": "ng serve ttth --port=4500",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
